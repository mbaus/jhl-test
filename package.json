{
  "name": "react-fullstack",
  "version": "0.0.0",
  "private": true,
  "description": "React Ts Fullstack Kurs",
  "license": "UNLICENSED",
  "scripts": {
    "build": "tsc -p tsconfig.node.json && vite build --emptyOutDir",
    "dev": "vite",
    "e2e": "npm run test:component",
    "e2e:headless": "npm run test:component:headless",
    "lint": "eslint --ext .js,.ts,.tsx src/main/webapp/app/**/*",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,js,ts,tsx,css,scss,vue,java,xml}\"",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest run --coverage",
    "test:component": "cypress open --config-file src/test/javascript/integration/cypress-config.ts",
    "test:component:headless": "cypress run --headless --config-file src/test/javascript/integration/cypress-config.ts",
    "test:test": "vitest",
    "test:watch": "vitest --"
  },
  "dependencies": {
    "@nextui-org/react": "1.0.0-beta.12",
    "axios": "1.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.2"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "2.2.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.14.1",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/ws": "8.5.4",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "^5.54.0",
    "@vitejs/plugin-react": "3.1.0",
    "@vitest/coverage-istanbul": "0.28.5",
    "cypress": "12.6.0",
    "eslint": "^8.35.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "8.0.3",
    "jsdom": "21.1.0",
    "lint-staged": "13.1.2",
    "prettier": "^2.8.4",
    "prettier-plugin-java": "2.0.0",
    "prettier-plugin-packagejson": "2.4.3",
    "sass": "1.58.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vitest": "0.28.5",
    "vitest-sonar-reporter": "0.3.5"
  },
  "engines": {
    "node": ">=18.14.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
